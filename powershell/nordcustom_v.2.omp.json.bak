{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\u256d\u2500\ue0b6",
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "foreground": "#4C566A",
          "background": "#E5E9F0",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " \uf07b </>{{ .Path }} ",
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b0",
          "background": "#81A1C1",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b0",
          "background": "#A3BE8C",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#D08770{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#EBCB8B{{ end }}",
            "{{ if gt .Ahead 0 }}#B48EAD{{ end }}",
            "{{ if gt .Behind 0 }}#B48EAD{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " \ueba2 {{ .FormattedMs }} ",
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b0",
          "background": "#B48EAD",
          "type": "executiontime",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf42e {{ end }}",
          "foreground": "#2E3440",
          "background": "#EBCB8B",
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf489 {{ .Name }} </>",
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b2",
          "background": "#D08770",
          "type": "shell",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 \ueb2d ",
          "foreground": "#242424",
          "powerline_symbol": "\ue0b2",
          "background": "#A3BE8C",
          "type": "battery",
          "style": "powerline",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#A3BE8C{{end}}",
            "{{if eq \"Discharging\" .State.String}}#A3BE8C{{end}}",
            "{{if eq \"Full\" .State.String}}#A3BE8C{{end}}"
          ],
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \ue641 {{ .CurrentDate | date .Format }} ",
          "foreground": "#2E3440",
          "background": "#81A1C1",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "foreground": "#ECEFF4",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "<#B48EAD>❯</><#A3BE8C>❯</><#EBCB8B>❯</> ",
          "foreground": "#8FBCBB",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "console_title_template": "{{.Folder}}{{if .Root}} :: root{{end}} :: {{.Shell}}",
  "version": 3
}
